pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh './ms-proposal.sh build cd'
            }
        }

        stage('Prepare env') {
            steps {
                sh './ms-proposal.sh prepare-env cd'
            }
        }

        stage('Prepare db') {
            steps {
                sh './ms-proposal.sh prepare-db cd'
            }
        }
    }
}
